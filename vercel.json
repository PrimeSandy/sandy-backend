{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/firebase-config",
      "dest": "/firebase-config"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/",
      "methods": ["GET"]
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
